package com.cdweb.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MainController {

    @GetMapping("/")
    public String home() {
        return "user/index";
    }

    @GetMapping("/login")
    public String login() {
        return "user/login-register";
    }
   
    
    @GetMapping("/admin")
    public String admin() {
        return "admin";
    }
}
